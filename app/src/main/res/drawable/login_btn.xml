<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:top="1dp"
        android:bottom="1dp"
        android:start="1dp"
        android:end="1dp">
        <shape android:shape="rectangle">
            <solid android:color="#FBA1B7"/>
            <corners android:radius="5dp"/>
            <stroke
                android:width="1dp"
                android:color="#FBA1B7" />
        </shape>
    </item>
</layer-list>